 @model Category

 <div>
     <h1>Category New</h1>
 </div>
 
 <form method="post">
     <div class="mb-3">
         <label>Category Name</label>
         <input type="text" class="form-control"/>
     </div>
     <div class="mb-3">
         <label>Display order</label>
         <input type="text" class="form-control"/>
     </div>
     <button type="submit">Create</button>
     <a asp-controller="Category" asp-action="Index" class="btn btn-secondary border form-control">Back</a>
 </form>

