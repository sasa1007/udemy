@model List<Category>

<h1>Category list</h1>
<table class="table table-bordered table-striped-columns">
    <thead>
    <tr>
        <th>
            Category Name
        </th>
        <th>
            Display order
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var obj in Model.OrderBy(u=>u.DisplayOrder))
    {
        <tr>
            <td>@obj.Name</td>
            <td>@obj.DisplayOrder</td>
        </tr>
    }
    </tbody>
</table>